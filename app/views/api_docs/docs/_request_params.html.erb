<table class="table">
  <% request_params.each do |key, value| %>
      <tr>
        <td><strong><%= key %></td>
        <td>
          <% if value.is_a?(Hash) %>
              <%= render :partial => 'request_params', :object => value %>
          <% else %>
              <%= value %>
          <% end %>
        </td>
      </tr>
  <% end %>
</table>     